{
  "name": "@streamware/nestjs-pulsar",
  "version": "0.0.6",
  "description": "NestJS npm package starter",
  "author": "John Biundo <johnfbiundo@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "rimraf dist && yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsplus/nestjs-package-starter"
  },
  "bugs": "https://github.com/nestjsplus/nestjs-package-starter",
  "peerDependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "3.2.2"
  },
  "dependencies": {
    "pulsar-client": "^1.11.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/testing": "10.3.8",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/supertest": "6.0.2",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.7",
    "supertest": "7.0.0",
    "ts-jest": "29.1.3",
    "ts-node": "10.9.2",
    "tsc-watch": "6.2.0",
    "tsconfig-paths": "4.2.0",
    "tslint": "6.1.3",
    "typescript": "5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
